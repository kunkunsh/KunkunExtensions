{
  "$schema": "https://schema.kunkun.sh/",
  "name": "ip-info",
  "version": "0.0.3",
  "module": "index.ts",
  "type": "module",
  "kunkun": {
    "name": "IP Info",
    "shortDescription": "Show your current IP Info and Search for IP Address",
    "longDescription": "Will display you IP Address and more information about this IP address, including Geo Location, ASN, ISP. You can also search for a specific IP.",
    "identifier": "ip-info",
    "permissions": [
      "clipboard:write-text",
      "fetch:all"
    ],
    "demoImages": [
      "./demo/ip-info.png"
    ],
    "icon": {
      "type": "iconify",
      "value": "mdi:ip-network"
    },
    "templateUiCmds": [
      {
        "name": "IP Info",
        "main": "dist/index.js",
        "description": "Display your current IP addres and detailed information. Search for other IP address.",
        "cmds": []
      }
    ],
    "customUiCmds": []
  },
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rimraf dist && rollup -c"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@kksh/api": "0.0.4-beta.0",
    "valibot": "^0.36.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rimraf": "^6.0.1",
    "rollup": "^4.18.1",
    "rollup-plugin-visualizer": "^5.12.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}