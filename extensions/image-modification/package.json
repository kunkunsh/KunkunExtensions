{
  "$schema": "https://schema.kunkun.sh",
  "name": "image-modification",
  "version": "0.0.2",
  "type": "module",
  "kunkun": {
    "name": "Image Modification",
    "shortDescription": "Image Modification",
    "longDescription": "Image Modification",
    "identifier": "image-modification",
    "permissions": [
      {
        "permission": "shell:deno:spawn",
        "allow": [
          {
            "path": "$EXTENSION/src/convert.ts",
            "ffi": "*",
            "read": "*"
          }
        ]
      },
      "shell:stdin-write",
      "system:fs",
      {
        "permission": "shell:deno:execute",
        "allow": [
          {
            "path": "$EXTENSION/src/convert.ts"
          }
        ]
      }
    ],
    "demoImages": [],
    "icon": {
      "type": "iconify",
      "value": "ph:image-bold"
    },
    "customUiCmds": [],
    "templateUiCmds": [
      {
        "name": "Convert Image Format",
        "main": "dist/index.js",
        "cmds": []
      }
    ]
  },
  "scripts": {
    "dev": "bun build.ts dev",
    "build": "bun build.ts"
  },
  "dependencies": {
    "@kksh/api": "0.0.12-beta.4"
  },
  "devDependencies": {
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "files": [
    "./dist",
    ".gitignore"
  ]
}