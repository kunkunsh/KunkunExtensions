		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@types/node": "^20.14.12",
		"@vitejs/plugin-vue": "^5.0.5",
		"autoprefixer": "^10.4.19",
		"postcss": "^8.4.38",
		"tailwindcss": "^3.4.6",
		"typescript": "^5.2.2",
